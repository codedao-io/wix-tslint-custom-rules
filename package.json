{
  "name": "wix-tslint-custom-rules",
  "version": "1.0.7",
  "description": "",
  "main": "wix-tslint-custom-rules.json",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "release": "wnpm-release --no-shrinkwrap"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1",
    "wnpm-ci": "^6.2.55"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@types/node": "^10.12.18",
    "tslint": "^5.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/**/*.spec.(ts|tsx)"
    ]
  }
}
